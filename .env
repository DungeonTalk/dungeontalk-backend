# PostgreSQL
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/dungeondb
SPRING_DATASOURCE_USERNAME=root
SPRING_DATASOURCE_PASSWORD=1234
SPRING_DATASOURCE_DRIVER_CLASS_NAME=org.postgresql.Driver

# MongoDB
SPRING_DATA_MONGODB_HOST=localhost
SPRING_DATA_MONGODB_PORT=27017
SPRING_DATA_MONGODB_DATABASE=dungeondb
SPRING_DATA_MONGODB_USERNAME=admin
SPRING_DATA_MONGODB_PASSWORD=1234
SPRING_DATA_MONGODB_AUTHENTICATION_DATABASE=admin

# Redis (Valkey)
SPRING_SESSION_STORE_TYPE=redis
SPRING_SESSION_REDIS_HOST=localhost
SPRING_SESSION_REDIS_PORT=6379

SPRING_CACHE_TYPE=redis
SPRING_CACHE_REDIS_HOST=localhost
SPRING_CACHE_REDIS_PORT=6380

# JPA
SPRING_JPA_HIBERNATE_DDL_AUTO=update
SPRING_JPA_SHOW_SQL=true
SPRING_JPA_HIBERNATE_FORMAT_SQL=true

# JWT
JWT_
JWT_ACCESS_EXPIRATION=604800000
JWT_REFRESH_EXPIRATION=604800000
